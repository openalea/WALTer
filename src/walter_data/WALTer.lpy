# Setting a delay of floral transition of tiller according to the cohort number
def transiflo_shift(tiller, geno):
  cohort_num = cohort_number(tiller)
  if cohort_num == 1:
    return 10
  elif cohort_num > 1:
    return (cohort_num-1) * (Psi_FT * Pls[geno])
  else:
    return 0


