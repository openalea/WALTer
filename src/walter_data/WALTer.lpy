      peduncle_contrib = max(0,(delta_H[num_plt][axis] - L_E[genotype_map[num_plt]]) * pi/2 * d_P[genotype_map[num_plt]])
      
      if delta_H[num_plt][axis] <= 0:
        tiller_surface[(num_plt,axis)] = leaf_contrib + stem_contrib
        ear_contrib = 0
