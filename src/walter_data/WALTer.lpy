  surface_plante = dict.fromkeys(surface_plante.keys(), 0)
  surface_plante_visible = dict.fromkeys(surface_plante_visible.keys(), 0)
  
  # Computation of GAI
  for org_id in range(len(lstring)):
    org = lstring[org_id]
