      Hcol_max[num_plt][axis] = Hcol_max[num_plt][axis] + Sh_max_temp
      Hcol_dead[num_plt][axis] =  Hcol_dead[num_plt][axis] + Sh_dead_max
      delta_H[num_plt][axis] = max(0, delta_H[num_plt][axis] - Sh_max_temp)
      
