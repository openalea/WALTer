#Phyllochron adjustment
def phyll_adjust(geno):
  if sowing_DOY >= SDSA:
    phyllo = Phl[geno]
  else:
    phyllo = Phl[geno] * (1 - Rp * min(sowing_DOY,SDWS))
  return phyllo


