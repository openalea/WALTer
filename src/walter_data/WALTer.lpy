          else:
            StA.Ln_pot = Lmax - (Lmax - StA.var_Lmin) * StA.vern_prog
            if StA.pn >= StA.Ln_pot:
              StA.vrn = True
              StA.var_Lmin = max((StA.Ln_pot + StA.pn)/2.0, StA.var_Lmin)
