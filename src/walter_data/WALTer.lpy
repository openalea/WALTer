# Number of grains produced
def calc_grain_nb(T, PAR):
  nb_epi_msqu = 592
  photothermal_quotient = PAR/T
  grain_nb =  (2600 + 8510 * photothermal_quotient)/nb_epi_msqu
  return grain_nb

def age_phyto_seed(num_phyto, N_p_s, geno):
  return (N_p_s - num_phyto) * Pls[geno]



